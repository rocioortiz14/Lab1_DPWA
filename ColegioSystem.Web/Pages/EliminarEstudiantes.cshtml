@page
@model ColegioSystem.Web.Pages.EliminarEstudiantesModel
@{
}
<jumbotrom>
    <h1>
        Eliminar de Estudiante
    </h1>
</jumbotrom>
<form method="post">
    <input type="hidden" asp-for="Estudiantes.Id" />
    <div asp-validation-summary="ModelOnly"></div>
    <div class="form-group">
        <label asp-for="Estudiantes.NIE"></label>
        <input type="text" class="form-control" asp-for="Estudiantes.NIE" placeholder="Ingrese Nombre del Docente">
        <span class="text-danger" asp-validation-for="Estudiantes.NIE"></span>
    </div>

    <div class="form-group">
        <label asp-for="Estudiantes.Nombre"></label>
        <input type="text" class="form-control" asp-for="Estudiantes.Nombre" placeholder="Ingrese Nombre del Docente">
        <span class="text-danger" asp-validation-for="Estudiantes.Nombre"></span>
    </div>

    <div class="form-group">
        <label asp-for="Estudiantes.Activo"></label>
        <input type="text" class="form-control" asp-for="Estudiantes.Activo">
    </div>
    <jumbotrom>
        <h3>
            Esta seguro de eliminar este registro
        </h3>
    </jumbotrom>
     <div class="form-group">
        <button type="submit" class="btn btn-sm btn-primary">Eliminar</button>   <a class="btn btn-sm btn-danger" asp-area="" asp-page="/Estudiantes">Cancelar</a>
    </div>
</form>
